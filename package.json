{
  "name": "wp-starterpack",
  "version": "1.0.0",
  "description": "Just another WordPress starter theme.",
  "main": "index.js",
  "scripts": {
    "start": "webpack --mode development --watch",
    "build": "webpack --mode production",
    "clean": "rm -r public/* & rm -r *.map",
    "eslint": "eslint \"src/**/*.{js,jsx}\" --quiet",
    "eslint:fix": "eslint \"src/**/*.{js,jsx}\" --quiet --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,scss}\"",
    "stylelint": "stylelint \"src/**/*.{css,scss}\"",
    "stylelint:fix": "stylelint \"src/**/*.{css,scss}\" --fix",
    "wp-pot": "wp-pot --src '*.php' --dest-file 'languages/_s.pot' --domain '_s'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/NSideas/wordpress-starterpack"
  },
  "keywords": [
    "wordpress",
    "webpack"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/NSideas/wordpress-starterpack/issues"
  },
  "homepage": "https://github.com/NSideas/wordpress-starterpack#readme",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "autoprefixer": "^9.7.6",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^8.1.0",
    "browser-sync": "^2.26.7",
    "browser-sync-webpack-plugin": "^2.2.2",
    "css-loader": "^1.0.1",
    "css-mqpacker": "^7.0.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^3.6.0",
    "eslint-config-wordpress": "^2.0.0",
    "eslint-loader": "^2.2.1",
    "eslint-plugin-prettier": "^2.7.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^2.0.0",
    "img-loader": "^3.0.1",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.13.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.19.1",
    "sass-loader": "^7.3.1",
    "style-loader": "^0.23.1",
    "stylelint": "^9.10.1",
    "stylelint-config-recommended-scss": "^3.3.0",
    "stylelint-config-wordpress": "^13.1.0",
    "stylelint-order": "^1.0.0",
    "stylelint-scss": "^3.17.0",
    "stylelint-webpack-plugin": "^0.10.5",
    "svg-sprite-loader": "^4.2.5",
    "svgo": "^1.3.0",
    "svgo-loader": "^2.2.1",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "wp-pot-cli": "^1.4.1"
  },
  "dependencies": {
    "@babel/polyfill": "^7.8.7",
    "node-gyp": "^4.0.0"
  }
}
